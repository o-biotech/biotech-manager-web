export function EaCManageSourceForm(props: EaCManageSourceFormProps) {
  const actionOptions: DataLookup[] = [
    {
      Lookup: "configure",
      Name: "Configure",
    },
    {
      Lookup: "fork",
      Name: "Fork",
    },
    // {
    //   Lookup: 'import',
