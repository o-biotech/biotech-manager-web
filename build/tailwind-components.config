          (displayStyle & DisplayStyleTypes.Top) === DisplayStyleTypes.Top
            ? "-:justify-start -:items-start"
            : undefined,
          (displayStyle & DisplayStyleTypes.Bottom) === DisplayStyleTypes.Bottom
